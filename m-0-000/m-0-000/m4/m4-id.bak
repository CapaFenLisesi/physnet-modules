\begin{IdItems}
\IdTitle{Taylor's Polynomial Approximation for Functions}

\IdAuthor{P.\,Signell, Dept.\,of Physics and Astronomy, Mich.\,State Univ}

\IdVersEval{2/22/99}{0}

\begin{Hours/Pages} \HR{1} \TX{4} \PS{2} \ME{2} \end{Hours/Pages}

\begin{InputSkills}
\item [1.]  Formally differentiate polynomial, transcendental, and
exponential functions \prrqone{0-1}.
\end{InputSkills}

\begin{KnowledgeSkills}
\item [K1.] Starting with Taylor's series, derive the formal expressions for
the finite difference approximations to the first and second derivatives.
\end{KnowledgeSkills}

\begin{RuleApplicationSkills}
\item [R1.] Expand any given common function in a Taylor's series about any
given point.
\item [R2.] Evaluate the first and second derivatives of any given function
by numerical techniques.
\end{RuleApplicationSkills}

\begin{PostOptions}
